{
  "name": "react-redux-sandbox",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "main": "lib/app.js",
  "files": [
    "README.md",
    "package.json",
    "lib"
  ],
  "scripts": {
    "start": "npm-run-all server",
    "test": "npm-run-all -p eslint mocha",
    "server": "npm-run-all -p server:*",
    "server:api": "apimocker -c fixture/api/config.json",
    "server:dev": "NODE_ENV=development webpack-dev-server --inline --hot --host localhost --port 8088 --content-base dist",
    "build:dev": "NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config webpack.prod.config.js",
    "eslint": "NODE_PATH=src/js eslint './src/js/**/*.{js,jsx}'",
    "mocha": "NODE_ENV=test NODE_PATH=src/js:src/css:test mocha --compilers js:babel-register -r ./test/setups/*.js './test/**/**.test.js'",
    "prepublish": "npm-run-all build:prod"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "apimocker": "^0.5.1",
    "autoprefixer": "^7.1.1",
